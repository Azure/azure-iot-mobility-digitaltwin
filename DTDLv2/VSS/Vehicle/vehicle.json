        "name": "Brand",
        "description": "Vehicle brand or manufacturer",
        "schema": "string"
    },
    {
        "@type": "Property",
        "name": "Model",
        "description": "Vehicle model",
        "schema": "string"
    },
    {
        "@type": ["Property", "Year"],
